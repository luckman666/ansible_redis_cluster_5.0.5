# This is the default ansible 'hosts' file.
#
# It should live in /etc/ansible/hosts
#
#   - Comments begin with the '#' character
#   - Blank lines are ignored
#   - Groups of hosts are delimited by [header] elements
#   - You can enter hostnames or ip addresses
#   - A hostname/ip can be a member of multiple groups

# Ex 1: Ungrouped hosts, specify before any group headers.

## green.example.com
## blue.example.com
## 192.168.100.1
## 192.168.100.10

# Ex 2: A collection of hosts belonging to the 'webservers' group

## [webservers]
## alpha.example.org
## beta.example.org
## 192.168.1.100
## 192.168.1.110

# If you have multiple hosts following a pattern you can specify
# them like this:

## www[001:006].example.com

# Ex 3: A collection of database servers in the 'dbservers' group

## [dbservers]
## 
## db01.intranet.mydomain.net
## db02.intranet.mydomain.net
## 10.25.1.56
## 10.25.1.57

# Here's another example of host ranges, this time there are no
# leading 0s:

## db-[99:101]-node.example.com
[118]
192.168.1.118 ansible_ssh_user=root ansible_ssh_pass=root123.
[jdk8]
192.168.1.117 ansible_ssh_user=root ansible_ssh_pass=root123.
192.168.1.118 ansible_ssh_user=root ansible_ssh_pass=root123.
192.168.1.119 ansible_ssh_user=root ansible_ssh_pass=root123.

[zookeeper]
192.168.1.117 ansible_ssh_user=root ansible_ssh_pass=root123. myid=1
192.168.1.118 ansible_ssh_user=root ansible_ssh_pass=root123. myid=2
192.168.1.119 ansible_ssh_user=root ansible_ssh_pass=root123. myid=3

[redis]
192.168.1.117 ansible_ssh_user=root ansible_ssh_pass=root123. prot=7000 nodeip1=192.168.1.117 
192.168.1.118 ansible_ssh_user=root ansible_ssh_pass=root123. prot=7002 nodeip2=192.168.1.118
192.168.1.119 ansible_ssh_user=root ansible_ssh_pass=root123. prot=7004 nodeip3=192.168.1.119
